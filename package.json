{
  "name": "angular-metro-maps",
  "version": "1.0.0",
  "description": "Angular 2+ components for metro svg maps",
  "repository": {
    "type": "git",
    "url": "https://github.com/falendary/angular-metro-maps.git"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf .tmp && mkdir dist && mkdir .tmp && rimraf 'packages/**/*.ngfactory.ts'",
    "prebuild": "npm run clean",
    "build": "npm run lint && npm run scripts && npm run packagejson",
    "scripts": "npm run ngc:esm",
    "packagejson": "node ./scripts/create-package-json.js",
    "copyassets": "node ./scripts/copy-package-assets.js",
    "ngc:esm": "ngc -p tsconfig.json",
    "postngc:esm": "mkdir -p dist/ && cp -R .tmp/esm/* dist/ && rimraf packages/**/*.ngfactory.ts",
    "bundle": "npm run bundle:umd:core",
    "bundle:umd:core": "rollup -c rollup.core.config.js",
    "lint": "tslint -c tslint.json 'packages/**/*.ts'"
  },
  "bugs": {
    "url": "https://github.com/falendary/angular-metro-maps/issues"
  },
  "keywords": [
    "angular",
    "ng",
    "metro",
    "n2",
    "angular2",
    "underground",
    "subway"
  ],
  "author": "Sergey Zhitenev",
  "license": "MIT",
  "peerDependencies": {
    "@angular/common": "^4.0.0 || ^2.0.0",
    "@angular/core": "^4.0.0 || ^2.0.0",
    "@angular/http": "^4.0.0 || ^2.0.0"
  },
  "jspm": {
    "jspmNodeConversion": false,
    "dependencies": {
      "@angular/common": "^4.0.0 || ^2.0.0",
      "@angular/core": "^4.0.0 || ^2.0.0",
      "@angular/http": "^4.0.0 || ^2.0.0"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@ngtools/webpack": "^1.5.4",
    "@angular/tsc-wrapped": "4.0.0",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "^3.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rxjs": "^5.4.2",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.2",
    "webpack": "^3.4.1",
    "zone.js": "^0.8.14"
  }
}
