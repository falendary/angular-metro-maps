    "@angular/tsc-wrapped": "4.0.0",
    "@ngtools/webpack": "^1.5.4",
    "@types/gulp": "^4.0.4",
    "@types/merge2": "^0.3.30",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.2",
    "codelyzer": "^3.1.2",
